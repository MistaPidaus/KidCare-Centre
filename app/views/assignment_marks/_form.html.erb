<%= form_with(model: assignment_mark, local: true) do |form| %>
  <% if assignment_mark.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(assignment_mark.errors.count, "error") %> prohibited this assignment_mark from being saved:</h2>

      <ul>
      <% assignment_mark.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
